{
    // 필수
    "manifest_version": 2, // 고정
    "name": "walletsubi",
    "version": "1.0.0",

    // 추천
    "description": "지갑수",
    "icons":  { "16": "images/subibasic.png" } ,
    "default_locale": "ko",
    "sidebar_action": {
        "default_page":  "sidebar.html"  ,// 필수
        "default_icon": {                // 필수
            "16": "images/subibasic.png"
        },
        // 선택
        "default_title": "지갑수비대",
        "use_navigation_bar": true,      // 기본값 true
        "mobile_user_agent": true        // 기본값 true
  },
  "permissions": [
    "notifications",
         "storage",
         "tabs",
        "activeTab",
        "<all_urls>"
      ],
      "background": {
        "scripts": ["background.js" ],
         "persistent": false
    },
  "web_accessible_resources": [
        "css/*.css",
        "images/*.png",
        "fonts/*"
    ]

}
